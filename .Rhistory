RUTA = "C:/jurrea/CTO_501_2020/Obligaciones/02_AccionesSeguimientoInformacion/Historicos_DHIME/descargasDiciembre2021/primeraDescarga"
Etiquetas = "HistoricosNIVEL_H"
for (i in 1:length(Etiquetas)) {
Datos = list.files(paste0(RUTA,"/",Etiquetas[i]),pattern = ".data",full.names = T)
if (length(Datos) == 0) { next() }
consolidado = NULL
for (j in 1:length(Datos)) {
Archivo = read.table(Datos[j],sep = "|",header = T)
Codigo = as.numeric(substr(Datos[j],which(strsplit(Datos[j],"")[[1]] == "@")+1,which(strsplit(Datos[j],"")[[1]] == ".")-1))
Archivo$Codigo = Codigo
Archivo = Archivo[c("Codigo","Fecha","Valor")]
Archivo$Fecha = strptime(Archivo$Fecha,"%Y-%m-%d %H:%M:%S")
consolidado = rbind(consolidado,Archivo)
print(paste0("consolidado ",Codigo," (",j,"/",length(Datos),") | etiqueta ",Etiquetas[i]," (",i,"/",length(Etiquetas),")"))
}
save(consolidado,file = paste0(RUTA,"/",Etiquetas[i],".RData"))
}
